{
  "manifest_version": 2,
  "short_name": "SavBook",
  "name": "SavBook",
  "default_locale": "en",
  "theme_color": "#FFC400",
  "icons": [
    {
      "src": "img/launcher-icon-2x.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "img/launcher-icon-3x.png",
      "sizes": "144x144",
      "type": "image/png"
    },
    {
      "src": "img/launcher-icon-4x.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ],
  "start_url": "/#/books",
  "display": "standalone",
  "orientation": "portrait"
}
